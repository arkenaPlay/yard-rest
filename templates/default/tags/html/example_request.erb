<% if object.has_tag?(:example_request) %>
  <div class="examples">
		<div class="api_action_header">
    	Example Request
		</div>
		
    <% object.tags(:example_request).each do |tag| %>
    	<h4><%= htmlify_line(tag.name) %></h4>
      <pre class="api_codeblock"><%= html_syntax_highlight(tag.text, :plain) %></pre>
    <% end %>
  </div>
<% end %>