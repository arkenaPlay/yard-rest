<li>
  <span class="object_link">
    <a href="<%= url_for('_index.html') %>" title="Home">Home</a>
  </span>
</li>
<% n = 1 %>
<% @items.sort_by { |s| s.to_s }.each do |resource| %>
  <% resource.tags("url").each do |url_tag| %>
    <li class="<%= n %>">
      <%= linkify(resource, url_tag.object.namespace.to_s.gsub('::','/') + '/' + url_tag.text) %>
    </li>
  <% end %>
  <% n = n == 2 ? 1 : 2 %>
<% end %>
